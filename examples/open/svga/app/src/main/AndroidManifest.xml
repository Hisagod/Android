<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.example.ponycui_home.svgaplayer">

    <uses-permission android:name="android.permission.INTERNET" />
    <uses-permission android:name="android.permission.READ_EXTERNAL_STORAGE" />
    <uses-permission android:name="android.permission.WRITE_EXTERNAL_STORAGE" />

    <application
        android:name=".App"
        android:allowBackup="true"
        android:hardwareAccelerated="true"
        android:icon="@mipmap/ic_launcher"
        android:label="@string/app_name"
        android:networkSecurityConfig="@xml/network_security_config"
        android:theme="@style/AppTheme">
        <activity
            android:name=".MainActivity"
            android:exported="true">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        <activity android:name=".load.LoadFromAssetsActivity" />
        <activity
            android:name=".AnimationFromJavaCodeActivity"
            android:exported="true" />
        <activity android:name=".AnimationFromNetworkActivity" />
        <activity android:name=".AnimationWithDynamicImageActivity" />
        <activity android:name=".AnimationFromLayoutActivity" />
        <activity android:name=".AnimationFromClickActivity" />
        <activity android:name=".AnimationFromRecyclerViewActivity" />
        <activity android:name=".AnimationFromFragmentActivity" />
        <activity android:name=".AnimationFromLazyFragmentActivity" />
        <activity android:name=".AnimationGifActivity" />
        <activity android:name=".PngActivity" />
        <activity
            android:name=".add.AddJavaActivity"
            android:exported="true">

        </activity>
        <activity
            android:name=".use.UseQueueActivity"
            android:exported="true">

        </activity>
    </application>
</manifest>
