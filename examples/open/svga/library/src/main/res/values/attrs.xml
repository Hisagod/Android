<?xml version="1.0" encoding="utf-8"?>
<resources>
    <declare-styleable name="SVGAImageView">
        <attr name="loopCount" format="integer" />
        <attr name="svg_tag" format="string" />
        <attr name="svg_flip" format="boolean" />
        <attr name="endAnimShow" format="enum">
            <enum name="endFrame" value="0" />
            <enum name="startFrame" value="1" />
            <enum name="clear" value="2" />
        </attr>
    </declare-styleable>
</resources>